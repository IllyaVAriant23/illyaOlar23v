#include <stdio.h>

int main() {
    int d = 1352;
    int sum = d / 1000 + (d % 1000) / 100 + (d % 100) / 10 + d % 10;
    int product = (d / 1000) * ((d % 1000) / 100) * ((d % 100) / 10) * (d % 10);

    // Виводимо результат
    printf("Сума цифр: %d\n", sum);
    printf("Добуток цифр: %d\n", product);

    return 0;
}
